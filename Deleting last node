#include <stdio.h>
#include <stdlib.h>
struct node
{
    int data;
    struct node *link;
};

struct node* del_last(struct node *head)
{
    if(head == NULL)
        printf("List is already empty!");
    else if(head->link == NULL)
    {
        free(head);
        head = NULL;
    }
    /*else
    {
        struct node *temp = head;
        struct node *temp2 = head;
        while(temp->link != NULL)
        {
            temp2 = temp;
            temp = temp->link;
        }
        temp2->link = NULL;
        free(temp);
        temp = NULL;
    }*/
    else
    {
        struct node *temp = head;
        while(temp->link->link != NULL)
        {
            temp = temp->link;
        }
        free(temp->link);
        temp->link = NULL;
    }
    return head;
};
int main()
{
    struct node *head = malloc(sizeof(struct node));
    head-> data = 25;
    head->link = NULL;

    struct node *ptr = malloc(sizeof(struct node));
    ptr->data = 35;
    ptr->link = NULL;
    head->link = ptr;

    ptr = malloc(sizeof(struct node));
    ptr->data = 45;
    ptr->link = NULL;
    head->link->link = ptr;

    head = del_last(head);
    ptr = head;
    while(ptr != NULL)
    {
        printf("%d ", ptr->data);
        ptr = ptr->link;
    }
    return 0;
}
